
@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" href="~/css/home.css" type="text/css" asp-append-version="true" />
<div class="main-wrapper">
    <div class="banner">
        <img src="https://rubicmarketing.com/wp-content/uploads/2021/07/banner-quang-cao-facebook-3.jpg" alt="Fake"
            srcset="">
    </div>
    <div class="container">
        <div class="card-container pt-5 pb-5">
            
            @foreach (var item in Model)
            {
                <partial name="_ProductItem" model="item"></partial>
            } 
        </div>
    </div>
</div>

<script src="~/js/home.js" asp-append-version="true"></script>